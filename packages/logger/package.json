{
  "name": "@cenie/logger",
  "version": "0.0.1",
  "private": true,
  "description": "CENIE Platform Centralized Logging Package",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "react-server": "./src/index.server.ts",
      "edge-light": "./src/index.client.ts",
      "browser": "./src/index.client.ts",
      "worker": "./src/index.client.ts",
      "types": "./src/index.ts",
      "default": "./src/index.server.ts"
    },
    "./client": {
      "types": "./src/index.client.ts",
      "default": "./src/index.client.ts"
    },
    "./server": {
      "types": "./src/index.server.ts",
      "default": "./src/index.server.ts"
    },
    "./middleware": {
      "types": "./src/middleware/index.ts",
      "default": "./src/middleware/index.ts"
    },
    "./next": {
      "types": "./src/middleware/next.ts",
      "default": "./src/middleware/next.ts"
    },
    "./express": {
      "types": "./src/middleware/express.ts",
      "default": "./src/middleware/express.ts"
    },
    "./context": {
      "react-server": "./src/context/async-context.server.ts",
      "edge-light": "./src/context/async-context.client.ts",
      "browser": "./src/context/async-context.client.ts",
      "types": "./src/context/index.ts",
      "default": "./src/context/async-context.server.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "default": "./src/utils/index.ts"
    }
  },
  "scripts": {
    "lint": "eslint . --max-warnings 60",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf .turbo node_modules dist coverage"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "fast-safe-stringify": "^2.1.1"
  },
  "peerDependencies": {
    "express": "^4.18.0",
    "next": "^14.0.0 || ^15.0.0 || ^16.1.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "express": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.9.1",
    "eslint": "^9.38.0",
    "typescript": "^5.9.3"
  }
}
